@model Kollaborator.web.Models.ApplicationUser

<div class="single-user-wrapper">
    <div id="editorRow">
        @using (Html.BeginCollectionItem("users"))
        {
            <div class="form-group">
                @Html.LabelFor(p => p.UserName, htmlAttributes: new { @class = "col-md-2 control-label" })
                <div class="col-md-10">

                    @Html.EditorFor(p => p.UserName, new { htmlAttributes = new { @class = "form-control input-md" } })
                    <a href="#" class="deleteRow">delete</a>
                </div>
            </div>
        }
        

    </div>
</div>

